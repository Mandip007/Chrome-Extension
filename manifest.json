{
    "manifest_version": 2,
  
    "name": "Quick Launcher",
    "description": "Quick launch Quick Launcher",
    "version": "1.0.0",
    "icons": { "128": "mandiplogo128.png" },
    "browser_action": {
      "default_icon": "mandiplogo.png",
      "default_popup": "index.html"
    },
    "permissions": ["activeTab"]
  }